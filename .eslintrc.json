{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "error",
    "camelcase": "off",
    "no-restricted-syntax": "off",
    "consistent-return": "off"
  },
  "plugins": ["prettier"]
}
